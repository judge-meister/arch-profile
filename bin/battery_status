#!/bin/bash

status=$(cat /sys/class/power_supply/BAT0/status) 
capacity=$(cat /sys/class/power_supply/BAT0/capacity)

echo "${capacity}% ${status}"
echo "${capacity}%"
if [ "${capacity}" -le 15 ] && [ "${status}" == "Discharging" ]
then
  exit 33
elif [ "${capacity}" -le 25 ]
then
  echo "#FF8000"
fi
exit 0
